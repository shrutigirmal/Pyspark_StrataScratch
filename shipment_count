# Import your libraries
import pyspark
from pyspark.sql import functions as F 

# Start writing code
unique_ship=amazon_shipment.withColumn("date", F.date_format("shipment_date","yyyy-MM")).groupBy("date").agg(F.countDistinct("shipment_id","sub_id").alias("shipments"))

# To validate your solution, convert your final pySpark df to a pandas df
unique_ship.toPandas()
